    <mj-style>
      /* My utility classes */
      .stack-on-mobile {
        display: block !important;
        width: 100% !important;
        max-width: none !important;
      }

      @media only screen and (max-width: 480px) {
        .center-on-mobile > div {
          text-align: center !important;
        }
      }

      @media only screen and (min-width: 768px) {
        .stack-on-desktop {
          text-align: left !important;
        }
      }

      /* --- MY DARK MODE ARMOR --- */
      /* This explicitly forces my original colors to stay the same even if the client tries to invert them to dark mode */
      @media (prefers-color-scheme: dark) {
        /* Force Backgrounds */
        .force-white-bg > table, .force-white-bg { background-color: #FFF !important; }
        .force-offwhite-bg > table, .force-offwhite-bg { background-color: #FAF9F6 !important; }
        .force-grey-bg > table, .force-grey-bg { background-color: #FAFAFA !important; }
        .force-orange-bg > table, .force-orange-bg { background-color: orange !important; }
        
        /* Force Text Colors */
        .force-black-text > div, .force-black-text span, .force-black-text a { color: #000 !important; }
        .force-blue-text > div, .force-blue-text span, .force-blue-text a { color: #052A75 !important; }
        .force-orange-text > div, .force-orange-text span { color: #EA6D2F !important; }
      }

      /* My Outlook App Dark Mode Fallback */
      [data-ogsc] .force-white-bg > table, [data-ogsc] .force-white-bg { background-color: #FFF !important; }
      [data-ogsc] .force-offwhite-bg > table, [data-ogsc] .force-offwhite-bg { background-color: #FAF9F6 !important; }
      [data-ogsc] .force-grey-bg > table, [data-ogsc] .force-grey-bg { background-color: #FAFAFA !important; }
      [data-ogsc] .force-orange-bg > table, [data-ogsc] .force-orange-bg { background-color: orange !important; }
      
      [data-ogsc] .force-black-text > div, [data-ogsc] .force-black-text span, [data-ogsc] .force-black-text a { color: #000 !important; }
      [data-ogsc] .force-blue-text > div, [data-ogsc] .force-blue-text span, [data-ogsc] .force-blue-text a { color: #052A75 !important; }
      [data-ogsc] .force-orange-text > div, [data-ogsc] .force-orange-text span { color: #EA6D2F !important; }
    </mj-style>
  </mj-head>

